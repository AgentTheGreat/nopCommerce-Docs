{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "_site/**",
          "***/readme.md",
          "***/license.md"
        ],
        "group": "group1",
        "src": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg",
          "**/*.pdf"
        ],
        "exclude": [
          "**/obj/**",
          "_site/**",
          "_exported_templates/**"
        ],
        "group": "group1",
        "src": "."
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadata": {
      "_navPath": "/",
      "_navRel": "/",
      "_appLogoPath": "/logo.png",
      "feedback_github_repo": "nopSolutions/nopCommerce-Docs",
      "feedback_product_url": "https://github.com/nopSolutions/nopCommerce-Docs/blob/master/CONTRIBUTING.md",
      "feedback_system": "GitHub",
      "searchScope": [
        "nopCommerce-Docs"
      ]
    },
    "template": [
      "default",
      "templates/nopcommerce"
    ],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}